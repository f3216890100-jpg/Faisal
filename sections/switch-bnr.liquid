.switch-mobile-div {
    font-size: 14px;
    color: #fff;
    font-weight: 600;
}
.switch-logo-div-1 {
    display: flex;
    align-items: center;
        gap: 10px;
                justify-content: center;
}
.switch-mobile-div-1 {
