<style>
        .switch-bnr-flx {
        display: flex;
    }
        .switch-left-col
        {
            background-image:url({{ section.settings.switch-left-col |img_url:'master' }});
            width:50%;
                background-position: bottom right;
        background-repeat: no-repeat;
        background-size: cover;
            height: 80vh;
             position:relative;
             transition: all .3s
    ease-in-out;
        }

        .switch-right-col
        {
            background-image:url({{ section.settings.switch-right-col |img_url:'master' }});
                  width:50%;
                      background-position: bottom right;
        background-repeat: no-repeat;
        background-size: cover;
            height: 80vh;
            transition: all .3s
    ease-in-out;
        }
         .switch-left-col:hover{
           background-image:url({{ section.settings.switch-left-col-1 |img_url:'master' }});
        }
          .switch-right-col:hover{
           background-image:url({{ section.settings.switch-right-col-1 |img_url:'master' }});
        }
       .switch-text {
        font-size: 20px;
        font-weight: 600;
        color: #454545;
            margin: 0;
            position:relative;
    }
    .switch-price {
        text-align: center;
            padding: 6px 0 ;
    }
    .switch-span {
        font-size: 18px;
        font-weight: 500;
        color: #454545;
            text-decoration: line-through;
    }
    .switch-price-span {
        font-size: 28px;
        font-weight: 600;
        color: #454545;
            text-decoration: line-through;
    }
    .switch-left-content {
           position: absolute;
            top: 35%;
           max-width: 330px;
        text-align:center;
         margin: 0 0 0 8%;
    }
    .switch-right-content {
           position: absolute;
        top:25%;
        right:0;
          max-width: 330px;
        text-align:center;
     margin: 0 8% 0 0;
    }
    .switch-price {
        display: flex;
        flex-direction: column;
    }
    .switch-btn {
        font-size: 20px;
        font-weight: 600;
        background: #085a54;
        text-decoration: none;
        color: #fff;
        padding: 4px 25px;
        display: inline-block;
        border-radius: 50px;
    }
    .switch-logo-img {
        width: 100%;
           height: 35px;
        max-width: 35px;
        object-fit: cover;
    }
    .switch-div {
        font-size: 14px;
        font-weight: 500;
        color: #454545;
    }
    .switch-logo {
        display: flex;
        justify-content: space-between;
            margin-top: 25px;
    }
    .switch-mobile-container{
      display:none;
    }
    @media screen and (max-width:767px){
       .switch-mobile-container{
      display:block;
    }
    .switch-bnr-container{
      display:none;
    }
    .switch-mobile-img {
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  .switch-mobile-col {
      position: relative;
      overflow: hidden;
  }
  .switch-mobile-logo {
      width: 100%;
      height: 100%;
      max-width: 30px;
  }
  .switch-mobile-logo-1 {
      width: 100%;
      height: 100%;
      max-width: 30px;
  }
    .switch-mobile-content {
        position: absolute;
        top: 25%;
        display: flex;
        align-items: center;
        left: 60px;
                width: 300px;
    }s
     .switch-mobile-span {
        font-size: 18px;
        font-weight: 600;
        color: #454545;
            text-decoration: line-through;
    }

   .switch-mobile-span-1 {
