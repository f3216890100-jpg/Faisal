}
.switch-moible-span-3{
    font-size: 20px;
    font-weight: 600;
    color: #454545;
}
   .switch-mobile-button {
        position: absolute;
        bottom: 11%;
        left: 55px;
    }
.switch-bt {
    display: flex;
    flex-direction: column;
    row-gap: 10px;
}
.switch-mobile-btn {
    background: #085A54;
    text-decoration: none;
    color: #fff;
    font-size: 20px;
    font-weight: 600;
    padding: 5px 20px;
    display: inline-block;
    border-radius: 50px;
    text-align: center;
}
.switch-mobile-btn-1 {
    background: #085A54;
