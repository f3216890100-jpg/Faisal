<section class="switch-bnr-sec">
  <div class="switch-bnr-container">
    <div class="switch-bnr-flx">
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'LeftCol' %}
            <div class="switch-left-col">
              <div class="switch-left-content">
                <h1 class="switch-text">
                  {{ block.settings['switch-text'] }}
                </h1>
                <div class="switch-price">
                  <span class="switch-span">
                    {{ block.settings['switch-span'] }}
                  </span>
                  <span class="switch-price-span">
                    {{ block.settings['switch-price-span'] }}
                  </span>
                </div>
                <div class="switch-button">
                  <a href="" class="switch-btn">
                    {{ block.settings['switch-btn'] }}
                  </a>
                </div>
                <div class="switch-logo">
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img'] != blank %}
                      <img src="{{ block.settings.switch-logo-img |img_url:'master' }}" alt="" class="switch-logo-img">
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div'] }}
                    </div>
                  </div>
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img-1'] != blank %}
                      <img
                        src="{{ block.settings.switch-logo-img-1 |img_url:'master' }}"
                        alt=""
                        class="switch-logo-img"
                      >
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div-1'] }}
                    </div>
                  </div>
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img-2'] != blank %}
                      <img
                        src="{{ block.settings.switch-logo-img-2 |img_url:'master' }}"
                        alt=""
                        class="switch-logo-img"
                      >
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div-2'] }}
                    </div>
                  </div>
                </div>
              </div>
            </div>
        {% endcase %}
      {% endfor %}
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'RightCol' %}
            <div class="switch-right-col">
              <div class="switch-right-content">
                <h1 class="switch-text">
                  {{ block.settings['switch-text-1'] }}
                </h1>
                <div class="switch-price">
                  <span class="switch-span">
                    {{ block.settings['switch-span-1'] }}
                  </span>
                  <span class="switch-price-span">
                    {{ block.settings['switch-price-span-1'] }}
                  </span>
                </div>
                <div class="switch-button">
                  <a href="" class="switch-btn">
                    {{ block.settings['switch-btn-1'] }}
                  </a>
                </div>
                <div class="switch-logo">
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img-3'] != blank %}
                      <img
                        src="{{ block.settings.switch-logo-img-3 |img_url:'master' }}"
                        alt=""
                        class="switch-logo-img"
                      >
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div-3'] }}
                    </div>
                  </div>
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img-4'] != blank %}
                      <img
                        src="{{ block.settings.switch-logo-img-4 |img_url:'master' }}"
                        alt=""
                        class="switch-logo-img"
                      >
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div-4'] }}
                    </div>
                  </div>
                  <div class="switch-logo-image">
                    {% if block.settings['switch-logo-img-5'] != blank %}
                      <img
                        src="{{ block.settings.switch-logo-img-5 |img_url:'master' }}"
                        alt=""
                        class="switch-logo-img"
                      >
                    {% endif %}
                    <div class="switch-div">
                      {{ block.settings['switch-div-5'] }}
                    </div>
                  </div>
                </div>
              </div>
            </div>
        {% endcase %}
      {% endfor %}
    </div>
  </div>
  <div class="switch-mobile-container">
    <div class="switch-mobile-col">
      <div class="switch-mobile-image">
        {% if section.settings['switch-mobile-img'] != blank %}
          <img src="{{ section.settings.switch-mobile-img |img_url:'master' }}" alt="" class="switch-mobile-img">
        {% endif %}
      </div>
      <div class="switch-mobile-content">
        <div class="switch-mobile-price">
        <span class="switch-mobile-span">
          {{ section.settings['switch-mobile-span'] }}
        </span>
        <span class="switch-mobile-span-1">
          {{ section.settings['switch-mobile-span-1'] }}
        </span>
        </div>
          <div class="switch-mobile-price-1">
        <span class="switch-moible-span">
          {{ section.settings['switch-mobile-span-2'] }}
        </span>
        <span class="switch-moible-span-3">
          {{ section.settings['switch-mobile-span-3'] }}
        </span>
        </div>
      </div>
    </div>
    <div class="switch-mobile-button">
      <div class="switch-bt">
        <a href="" class="switch-mobile-btn">
          {{ section.settings['switch-mobile-btn'] }}
        </a>
        <a href="" class="switch-mobile-btn-1">
           {{ section.settings['switch-mobile-btn-1'] }}
        </a>
       
        
      </div>
      <div class="switch-logo-div">
        {% if section.settings['switch-mobile-logo'] != blank %}
          <img src="{{ section.settings.switch-mobile-logo |img_url:'master' }}" alt="" class="switch-mobile-logo">
        {% endif %}
        <div class="switch-mobile-div">
          {{ section.settings['switch-mobile-div'] }}
        </div>
      </div>
      <div class="switch-logo-div-1">
        {% if section.settings['switch-mobile-logo-1'] != blank %}
          <img src="{{ section.settings.switch-mobile-logo-1 |img_url:'master' }}" alt="" class="switch-mobile-logo-1">
        {% endif %}
        <div class="switch-mobile-div-1">
          {{ section.settings['switch-mobile-div-1'] }}
        </div>
      </div>
    </div>
  </div>
</section>
