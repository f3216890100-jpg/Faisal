                  src="{{ section.settings.sub-cust-lime-cart-img |img_url:'master' }}"
                  alt="{{ section.settings.sub-cust-lime-cart-img.alt | escape }}"
                  class="cust-lime-cart-img"
                >
              {% endif %}
            {% else %}
