<style>
    .cust-menu-col-div::after {
      content: "";
      position: absolute;
      background: url({{ section.settings.sub-background-img |img_url:'master' }});
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      z-index: -1;
      background-repeat: no-repeat;
      background-position: center;
      background-size: cover;
      border-radius: 12px;
      opacity: 0;
      transition: opacity .3s ease-in-out;
  }
  .cust-col-2-div-3 {
      position: relative;
      height: 145px;
      background-image: url({{ section.settings.sub-background-image |img_url:'master'}});
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
      border-radius: 12px;
      padding: 20px;

  }
  .cust-menu-2-title-div:hover {
        background-image: url({{ section.settings.sub-background-hover-image |img_url:'master'}});
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
  }
  .cust-menu-2-box {
      background: #fff;
      width: 33%;
      padding: 20px;
      position: relative;
      height: 300px;
      border-radius: 12px;
       background-image: url({{ section.settings.sub-background-image-1 |img_url:'master'}});
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
  }
</style>
