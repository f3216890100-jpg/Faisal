          <div class="cust-lime-logo">
            <div class="cust-lime-first-logo">
              {% if section.settings['sub-cust-lime-first-img'] != blank %}
                <img
                  src="{{ section.settings.sub-cust-lime-first-img |img_url:'master' }}"
                  alt="{{ section.settings.sub-cust-lime-first-img.alt | escape}}"
                  class="cust-lime-first-img"
