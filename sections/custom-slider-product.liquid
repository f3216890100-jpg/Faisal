                  <i class="fa-solid fa-chevron-down"></i>
                </div>
                <div class="cst-answer">
                  <div class="cst-desp">
                 {{ block.settings.sub-cst-desp-18 }}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="cst-ankr-div">
          <a href="#" class="cst-btn">
            {{ block.settings.sub-cst-btn }}
          </a>
        </div>
        <div class="cst-bt-div">
          <a href="#" class="cst-bt">
              {{ block.settings.sub-cst-bt }}
          </a>
          <i class="fa-solid fa-chevron-right"></i>
        </div>
        <div class="cst-content">
          <h4 class="cst-heading">
             {{ block.settings.sub-cst-heading }}
          </h4>
          <div class="cst-div">
             {{ block.settings.sub-cst-div }}
          </div>
        </div>
      </div>
      {% endcase %}
    {% endfor %}
    </div>
  </div>
</section>


