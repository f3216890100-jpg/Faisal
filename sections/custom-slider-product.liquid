                  <i class="fa-solid fa-chevron-down"></i>
                </div>
                <div class="cst-answer">
                  <div class="cst-desp">
                  {{ block.settings.sub-cst-desp-17 }}
                  </div>
                </div>
              </div>
              <div class="cst-faq">
                <div class="cst-quesrion">
                  <span class="cst-prod-faq-span-1">
                    {{ block.settings.sub-cst-prod-faq-span-7 }}
