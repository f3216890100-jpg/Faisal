<section class="cst-slider-sec">
  <div class="cst-slider-container">
    <div class="cst-slider-flx">
        {% for block in section.blocks %}
            {% case block.type %}
            {% when 'Slider' %}
      <div class="cst-slider-col">
        <div class="cst-prd-image-flx">
          <div class="slider-for">
            <div class="cst-slider-image">
                {% if block.settings.sub-cst-slider-img-1 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-1 |img_url:'master' }}" alt="" class="cst-slider-img-1">
              {% endif %}
            </div>
            <div class="cst-slider-image">
               {% if block.settings.sub-cst-slider-img-2 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-2 |img_url:'master' }}" alt="" class="cst-slider-img-1">
              {% endif %}
            </div>
            <div class="cst-slider-image">
              {% if block.settings.sub-cst-slider-img-3 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-3 |img_url:'master' }}" alt="" class="cst-slider-img-1">
              {% endif %}
            </div>
            <div class="cst-slider-image">
              {% if block.settings.sub-cst-slider-img-4 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-4 |img_url:'master' }}" alt="" class="cst-slider-img-1">
              {% endif %}
            </div>
          </div>
          <div class="slider-nav">
            <div class="cst-slider-image">
           {% if block.settings.sub-cst-slider-img-5 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-5 |img_url:'master' }}" alt="" class="cst-slider-img">
              {% endif %}
            </div>
            <div class="cst-slider-image">
             {% if block.settings.sub-cst-slider-img-6 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-6 |img_url:'master' }}" alt="" class="cst-slider-img">
              {% endif %}
            </div>
            <div class="cst-slider-image">
              {% if block.settings.sub-cst-slider-img-7 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-7 |img_url:'master' }}" alt="" class="cst-slider-img">
              {% endif %}
            </div>
            <div class="cst-slider-image">
             {% if block.settings.sub-cst-slider-img-8 !=blank %}
              <img src="{{ block.settings.sub-cst-slider-img-8 |img_url:'master' }}" alt="" class="cst-slider-img">
              {% endif %}
            </div>
          </div>
        </div>
      </div>
      {% endcase %}
    {% endfor %}
    {% for block in section.blocks %}
            {% case block.type %}
            {% when 'Content' %}
      <div class="cst-slider-col-1">
        <div class="cst-product-title">
       {{ block.settings.sub-cst-product-title-1 }}
        </div>
        <div class="cst-product-star-row">
          <div class="cst-prod-reviews">
            <span class="cst-prod-reviews-icon"><i class="fa-solid fa-star"></i></span>
            <span class="cst-prod-reviews-icon"><i class="fa-solid fa-star"></i></span>
            <span class="cst-prod-reviews-icon"><i class="fa-solid fa-star"></i></span>
            <span class="cst-prod-reviews-icon"><i class="fa-solid fa-star"></i></span>
            <span class="cst-prod-reviews-icon"><i class="fa-solid fa-star"></i></span>
          </div>
          <span class="reviews-text">
            {{ block.settings.sub-reviews-text }}
          </span>
        </div>
        <h5 class="cst-product-heading">
            {{ block.settings.sub-cst-product-heading }}
        </h5>
        <div class="cst-product-go-flx">
          <div class="cst-product-dot-flx">
            <div class="cst-product-dot"></div>
            <div class="cst-product-anker-flx">
              <span class="cst-product-anker-span">
                     {{ block.settings.sub-cst-product-anker-span }}
                <a class="cst-product-anker" href="#">
                    {{ block.settings.sub-cst-product-anker }}
                  <i class="fa-solid fa-chevron-right"></i>
                </a>
              </span>
            </div>
          </div>
          <div class="cst-product-value">
            {{ block.settings.sub-cst-product-value }}
          </div>
        </div>
        <div class="cst-product-go-out-flx">
          <div class="go-out-dot-flx">
            <div class="cst-product-go-out-dots"></div>
            <h5 class="cst-product-go-out-text">
                {{ block.settings.sub-cst-product-go-out-text }}
            </h5>
          </div>
          <div class="go-out-price">
            {{ block.settings.sub-go-out-price }}
          </div>
        </div>
        <h5 class="cst-product-heading">
                {{ block.settings.sub-cst-product-heading-1 }}
        </h5>
        <div class="cst-product-faq-div">
          <div class="cst-prod-faq">
            <div class="cst-prod-quesstion">
              <span class="cst-prod-faq-span">
                    {{ block.settings.sub-cst-prod-faq-span }}
              </span>
              <i class="fa-solid fa-chevron-down"></i>
            </div>
            <div class="cst-prod-faq-answer">
              <div class="cst-faq">
                <div class="cst-quesrion">
                  <span class="cst-prod-faq-span-1">
                    {{ block.settings.sub-cst-prod-faq-span-1 }}
                  </span>
                  <i class="fa-solid fa-chevron-down"></i>
                </div>
                <div class="cst-answer">
                  <div class="cst-desp">
                   {{ block.settings.sub-cst-desp }}
                  </div>
                  <div class="cst-desp">
                     {{ block.settings.sub-cst-desp-2 }}
                  </div>
                  <div class="cst-desp">
                     {{ block.settings.sub-cst-desp-3 }}
                  </div>
                  <div class="cst-desp">
                     {{ block.settings.sub-cst-desp-4 }}
